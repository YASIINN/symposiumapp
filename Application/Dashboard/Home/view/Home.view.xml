<mvc:View
 controllerName="symposiumapp.Application.Dashboard.Home.controller.Home" 
	height="100%"
	xmlns:mvc="sap.ui.core.mvc"
	 xmlns:l="sap.ui.layout" 
    xmlns:core="sap.ui.core" 
    xmlns:u="sap.ui.unified" 
	xmlns="sap.m">
	<Page showHeader="false"  enableScrolling="true">
		<content>
			     <IconTabBar id="idIconTabBarSeparatorNoIcon" select="changekey" expanded="true" class="sapUiResponsiveContentPadding">
                <items>
                    <IconTabFilter key="project" icon="sap-icon://accept" text=" PROJE SEÇ" iconColor="Positive" count="1">
                        	<Panel width="auto" class="sapUiResponsiveMargin" accessibleRole="Region">
							<headerToolbar>
							<Toolbar height="3rem">
								<Title text="Panel with picture"/>
							</Toolbar>
						</headerToolbar>
							<content>
                                    <VBox width="100%" alignItems="Stretch" justifyContent="Start">
                                <VBox alignItems="Stretch" justifyContent="Start" width="100%">
                                    <HBox alignItems="Center" justifyContent="Stretch" width="100%">
                                        <Label text="{i18n>ProjectName}" width="130px" />
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                            <Input width="100%" value="{/projectModel/0/pjnm}"/>
                                        </VBox>
                                    </HBox>
                                    <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Text width="130px" text="Position / Title"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                               	<Select forceSelection="false" width="100%" 
										items="{
											path: '/title'}">
						<core:Item key="{tid}" text="{titletxt}"/>
												</Select>  
                                        </VBox>
                                    </HBox>
                                    <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Label width="130px" text="{i18n>ProjectTechnologies}"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%" alignItems="Stretch" justifyContent="Stretch">
                                 <Input width="100%" value="{/projectModel/0/pjnm}"/>
                                        </VBox>
                                    </HBox>
                                    <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Text width="130px" text="Country"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                         	<Select forceSelection="false" width="100%"
							showSecondaryValues= "true"
										items="{
											path: '/AllCountry',
										sorter: { path: 'name' }
												}">
						<core:ListItem key="{key}" text="{name}" additionalText= "{key}"/>
												</Select>
                                        </VBox>
                                    </HBox>

                                    <HBox class="sapUiTinyMarginTop" width="100%" alignItems="Start" justifyContent="Stretch">
                                        <VBox width="100%" alignItems="Stretch" justifyContent="Start">
                                            <HBox alignItems="Center" justifyContent="Stretch" width="100%">
                                                <Text width="130px" text="{i18n>RelatedCourses}"/>
                                                <Label text=":" width="10px" />
                                                <VBox width="100%" alignItems="Stretch" justifyContent="Start">
                                                         <Input width="100%" value="{/projectModel/0/pjnm}"/>
                                                </VBox>
                                            </HBox>
                                        </VBox>
                                    </HBox>
                                                  <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Text width="130px" text="{i18n>ProjectQuotas}"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                        <Input value="{/projectModel/0/pjquota}"/>
                                        </VBox>
                                    </HBox>
                                        <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Text width="130px" text="{i18n>ProjectQuotas}"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                        <Input value="{/projectModel/0/pjquota}"/>
                                        </VBox>
                                    </HBox>
                                    <HBox width="100%" alignItems="Center" justifyContent="End" class="sapUiTinyMarginTop">
                                        <Button  press="onSaveData" type="Default" text="{i18n>Save}" tooltip="{i18n>Save}" />
                                    </HBox>
                                </VBox>
                        </VBox>
                        		</content>
	</Panel>
		<Panel width="auto" class="sapUiResponsiveMargin" accessibleRole="Region">
		<headerToolbar>
			<Toolbar height="3rem">
				<Title text="Panel with picture"/>
			</Toolbar>
		</headerToolbar>
		<content>
			     <VBox width="100%" alignItems="Stretch" justifyContent="Start">
                                <VBox alignItems="Stretch" justifyContent="Start" width="100%">
                                    <HBox alignItems="Center" justifyContent="Stretch" width="100%">
                                        <Label text="{i18n>ProjectName}" width="130px" />
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                            <Input width="100%" value="{/projectModel/0/pjnm}"/>
                                        </VBox>
                                    </HBox>
                                    <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Text width="130px" text="{i18n>ProjectShortDescription}"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                            <TextArea width="100%" height="100%" value="{/projectModel/0/psd}" />
                                        </VBox>
                                    </HBox>
                                    <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Label width="130px" text="{i18n>ProjectTechnologies}"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%" alignItems="Stretch" justifyContent="Stretch">
                                         <TextArea width="100%" height="100%" value="{/projectModel/0/ptek}" />
                                        </VBox>
                                    </HBox>
                                    <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Text width="130px" text="{i18n>ProjectTerm}"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                          
                                        </VBox>
                                    </HBox>

                                    <HBox class="sapUiTinyMarginTop" width="100%" alignItems="Start" justifyContent="Stretch">
                                        <VBox width="100%" alignItems="Stretch" justifyContent="Start">
                                            <HBox alignItems="Center" justifyContent="Stretch" width="100%">
                                                <Text width="130px" text="{i18n>RelatedCourses}"/>
                                                <Label text=":" width="10px" />
                                                <VBox width="100%" alignItems="Stretch" justifyContent="Start">


                                                    <MultiComboBox  selectionChange="handleSelectionChange" selectionFinish="handleSelectionFinish" items="{
				path: '/allLesson'
			}">
                                                        <core:Item key="{lid}" text="{lnm}" />
                                                    </MultiComboBox>
                                                </VBox>
                                                <Button icon="sap-icon://add" tooltip="{i18n>AddCourse}" type="Transparent" press="addless"/>

                                            </HBox>
                                        </VBox>
                                    </HBox>
                                                  <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Text width="130px" text="{i18n>ProjectQuotas}"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                        <Input value="{/projectModel/0/pjquota}"/>
                                        </VBox>
                                    </HBox>
                                    <HBox width="100%" alignItems="Center" justifyContent="End" class="sapUiTinyMarginTop">
                                        <Button  press="onSaveData" type="Default" text="{i18n>Save}" tooltip="{i18n>Save}" />
                                    </HBox>
                                </VBox>
                        </VBox>
	
		</content>
	</Panel>
		<Panel width="auto" class="sapUiResponsiveMargin" accessibleRole="Region">
		<headerToolbar>
			<Toolbar height="3rem">
				<Title text="Panel with picture"/>
			</Toolbar>
		</headerToolbar>
		<content>
	     <VBox width="100%" alignItems="Stretch" justifyContent="Start">
                                <VBox alignItems="Stretch" justifyContent="Start" width="100%">
                                    <HBox alignItems="Center" justifyContent="Stretch" width="100%">
                                        <Label text="{i18n>ProjectName}" width="130px" />
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                            <Input width="100%" value="{/projectModel/0/pjnm}"/>
                                        </VBox>
                                    </HBox>
                                    <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Text width="130px" text="{i18n>ProjectShortDescription}"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                            <TextArea width="100%" height="100%" value="{/projectModel/0/psd}" />
                                        </VBox>
                                    </HBox>
                                    <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Label width="130px" text="{i18n>ProjectTechnologies}"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%" alignItems="Stretch" justifyContent="Stretch">
                                         <TextArea width="100%" height="100%" value="{/projectModel/0/ptek}" />
                                        </VBox>
                                    </HBox>
                                    <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Text width="130px" text="{i18n>ProjectTerm}"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                          
                                        </VBox>
                                    </HBox>

                                    <HBox class="sapUiTinyMarginTop" width="100%" alignItems="Start" justifyContent="Stretch">
                                        <VBox width="100%" alignItems="Stretch" justifyContent="Start">
                                            <HBox alignItems="Center" justifyContent="Stretch" width="100%">
                                                <Text width="130px" text="{i18n>RelatedCourses}"/>
                                                <Label text=":" width="10px" />
                                                <VBox width="100%" alignItems="Stretch" justifyContent="Start">


                                                    <MultiComboBox  selectionChange="handleSelectionChange" selectionFinish="handleSelectionFinish" items="{
				path: '/allLesson'
			}">
                                                        <core:Item key="{lid}" text="{lnm}" />
                                                    </MultiComboBox>
                                                </VBox>
                                                <Button icon="sap-icon://add" tooltip="{i18n>AddCourse}" type="Transparent" press="addless"/>

                                            </HBox>
                                        </VBox>
                                    </HBox>
                                                  <HBox alignItems="Center" justifyContent="Stretch" width="100%" class="sapUiTinyMarginTop">
                                        <Text width="130px" text="{i18n>ProjectQuotas}"/>
                                        <Label text=":" width="10px" />
                                        <VBox width="100%">
                                        <Input value="{/projectModel/0/pjquota}"/>
                                        </VBox>
                                    </HBox>
                                    <HBox width="100%" alignItems="Center" justifyContent="End" class="sapUiTinyMarginTop">
                                        <Button  press="onSaveData" type="Default" text="{i18n>Save}" tooltip="{i18n>Save}" />
                                    </HBox>
                                </VBox>
                        </VBox>

		</content>
	</Panel>
	                    </IconTabFilter>
                    <IconTabSeparator icon=""/>
                    <IconTabFilter key="point" icon="sap-icon://add" iconColor="Neutral" text=" NOT GİR" count="2">
                        <Table inset="false" items="{
                                            path: '/userpoint'
                                        }">
                            <infoToolbar>
                                <Toolbar active="ture">
                                    <Label text="Seçtiğiniz Projelerde Bulunan Derslerin Notlarını Giriniz"/>
                                </Toolbar>
                            </infoToolbar>
                            <columns>
                                <Column width="3rem">
                                    <Text text="Ders Adı" />
                                </Column>
                                <Column width="3rem">
                                    <Text text="Ders Notu" />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem>
                                    <cells>
                                        <ObjectIdentifier text="{lnm}"/>
                                        <Input  width ="100px" value="{upnt}"/>
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
                        <HBox alignItems="Center" justifyContent="End" width="100%" class="sapUiTinyMarginTop">
                            <Button text="Tamam" press="userpointcheck"/>
                        </HBox>
                    </IconTabFilter>
                    <IconTabSeparator icon=""/>
                    </items>
                </IconTabBar>
		</content>
	</Page>
</mvc:View>