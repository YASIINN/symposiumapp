<View
    controllerName="symposiumapp.MasterApp"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:tnt="sap.tnt"
    xmlns:l="sap.ui.layout">
    <l:FixFlex>
        <l:fixContent>
           <VBox alignItems="Stretch" justifyContent="Stretch" height="20%" width="100%">
                    <HBox>
                        <Image src="http://localhost/symposiumapp/resources/img/banner.png" densityAware="false" width="100%" height="120px">
                            <layoutData>
                                <FlexItemData growFactor="1" />
                            </layoutData>
                        </Image>
                    </HBox>
                    <VBox>
        	<Title id="WrappingTitle" text="REGISTRATION-SUBMISSON - PAYMENT MODULE"  class="sapUiResponsiveMargin"/>

                    </VBox>
                </VBox>
        </l:fixContent>
        <l:flexContent>
            <App id="masterView" />
        </l:flexContent>
    </l:FixFlex>
</View>
